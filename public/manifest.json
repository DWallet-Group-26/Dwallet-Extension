{
	"name": "Dwallet Extensions",
	"description": "Cryto Wallet with 2FA",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
		"title": "Dwallet"
	},
	"icons": {
		"16": "logo192.png",
		"48": "logo192.png",
		"128": "logo192.png"
	},
	"background": {
		"scripts": ["./static/js/background.js"]
	},
	"permissions": ["storage"],
	"optional_permissions": ["<all_urls>"]
}
